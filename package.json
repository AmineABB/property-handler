{
  "name": "property-handler",
  "version": "0.0.0-semantically-released",
  "description": "Object property handler",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "build": "webpack --env.prod -p",
    "build:dev": "webpack --env.dev",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "watch:test": "mocha --watch",
    "lint": "eslint src",
    "coverage": "cat ./coverage/lcov.info | codecov",
    "validate": "npm-run-all --parallel lint test build",
    "release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmineABB/property-handler"
  },
  "files": [
    "dist"
  ],
  "author": "Amine Abbi <abbi.amine@gmail.com> (http://github.com/AmineABB/)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.4",
    "babel-plugin-istanbul": "4.1.6",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "clean-webpack-plugin": "0.1.19",
    "codecov": "3.0.2",
    "commitizen": "2.10.1",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.12.0",
    "ghooks": "2.0.4",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "semantic-release": "15.5.4",
    "validate-commit-msg": "2.14.0",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.3",
    "webpack-config-utils": "2.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run validate",
      "commit-msg": "validate-commit-msg"
    }
  }
}
